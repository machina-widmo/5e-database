        "descriptor",
        "full_name",
        "skills"
    ],
    "properties": {
        "descriptor": {
            "$ref": "DatabaseEntryDescriptor.schema.json"
        },
        "full_name": {
            "type": "string"
        },
        "skills": {
            "type": "array",
            "items": {
                "$ref": "DatabaseEntryReference.schema.json"
            }
        }
    }
}