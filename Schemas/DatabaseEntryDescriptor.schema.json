{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "DatabaseEntryDescriptor.schema.json",
    "title": "DatabaseEntryDescriptor",
    "type": "object",
    "required": [
        "index",
        "name",
        "desc",
        "url"
    ],
    "properties": {
        "index": {
            "type": "number"
        },
        "name": {
            "type": "string"
        },
        "desc": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "url": {
            "type": "string"
        }
    }
}