    "type": "object",
    "required": [
        "descriptor",
        "class",
        "subclass",
        "level"
    ],
    "properties": {
        "descriptor": {
            "$ref": "DatabaseEntryDescriptor.schema.json"
        },
        "class": {
            "oneOf": [
                {
                    "type": "null"
                },
                {
                    "$ref": "DatabaseEntryReference.schema.json"
                }
            ]
        },
        "subclass": {
            "oneOf": [
                {
                    "type": "null"
                },
                {
                    "$ref": "DatabaseEntryReference.schema.json"
                }
            ]
        },
        "level": {
            "type": "number"
        }
    }
}