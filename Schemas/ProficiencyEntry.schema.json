{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "ProficiencyEntry.schema.json",
    "title": "ProficiencyEntry",
    "type": "object",
    "required": [
        "descriptor",
        "proficiency_type",
        "classes",
        "races"
    ],
    "properties": {
        "descriptor": {
            "$ref": "DatabaseEntryDescriptor.schema.json"
        },
        "proficiency_type": {
            "type": "string"
        },
        "classes": {
            "type": "array",
            "items": {
                "$ref": "DatabaseEntryReference.schema.json"
            }
        },
        "races": {
            "type": "array",
            "items": {
                "$ref": "DatabaseEntryReference.schema.json"
            }
        }
    }
}